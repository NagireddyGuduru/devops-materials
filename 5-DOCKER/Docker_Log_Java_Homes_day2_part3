[HM[m
[HM       NETWORK-NAME[m
[HM       [[01;38;5;208m--ipam-opt[0m=[04;38;5;111mmap[][0m] [[01;38;5;208m--ipv6[0m] [[01;38;5;208m--label[0m[=[04;38;5;111m[][0m]] [[01;38;5;208m-o[0m|[01;38;5;208m--opt[0m=[04;38;5;111mmap[][0m] [[01;38;5;208m--subnet[0m=[04;38;5;111m[][0m][m
